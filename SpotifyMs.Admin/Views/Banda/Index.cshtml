@model IEnumerable<SpotifyMs.Aplication.Streaming.Dto.BandaDto>
<div>
    <a class="btn btn-primary" role="button" href="/Banda/Criar">Criar Banda</a>
</div>

<div>
    <table class="table table-hover">
        <thead>
            <tr>
                <td>Id</td>
                <td>Nome</td>
                <td>Descricao</td>
                <td>Backdrop</td>                
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Nome</td>
                    <td>@item.Descricao</td>
                    <td>@item.Backdrop</td>
                    <td></td>
                </tr>
            }
        </tbody>
    </table>
