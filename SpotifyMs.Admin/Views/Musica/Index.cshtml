@model IEnumerable<SpotifyMs.Aplication.Streaming.Dto.MusicaDto>
<div>
    <a class="btn btn-primary" role="button" href="/Musica/Criar">Criar Música</a>
</div>

<div>
    <table class="table table-hover">
        <thead>
            <tr>
                <td>Id</td>
                <td>Nome</td>
                <td>Duracao</td>               
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Nome</td>
                    <td>@item.Duracao</td>
                    <td></td>
                </tr>
            }
        </tbody>
    </table>

