@model IEnumerable<SpotifyMs.Aplication.Streaming.Dto.MusicaDto>
@{
    ViewData["Title"] = "Listagem de Músicas";
}
<h1>@ViewData["Title"]</h1>


<div>
    <table class="table table-hover">
        <thead>
            <tr>
                <td>Nome</td>
                <td>Duracao</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Nome</td>
                    <td>@item.Duracao</td>
                    <td></td>
                </tr>
            }
        </tbody>
    </table>

